{
  "$schema": "./ui/node_modules/oxfmt/configuration_schema.json",
  "singleQuote": true,
  "experimentalSortImports": {
    "newlinesBetween": false,
  },
  "experimentalSortPackageJson": {
    "sortScripts": true,
  },
  "experimentalTailwindcss": {
    "stylesheet": "ui/src/App.css",
  },
  "ignorePatterns": [
    "coverage/",
    "dist/",
    "pnpm-lock.yaml",
    "ui/index.html",
    "ui/src/translations/",
  ],
}
